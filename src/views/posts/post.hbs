<article class="grid-container {{type}}">
	<header id="lede" class="main">
		<h1>{{title}}</h1>
		<time datetime="{{roboDate published}}">{{scriptDate published}}</time>
	</header>
	<section class="main writing">
		{{marked content}}
		<a href="/posts" id="back"><small>Back to posts</small></a>
	</section>
</article>
